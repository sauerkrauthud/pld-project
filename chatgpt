import csv

# Open and read the CSV file
with open('courses.csv', mode='r') as file:
    reader = csv.DictReader(file, delimiter='\t')

    # Separate undergraduate and graduate courses
    undergraduates = []
    graduates = []

    for row in reader:
        if row['Level'] == 'U':
            undergraduates.append(row)
        elif row['Level'] == 'G':
            graduates.append(row)

    # Sort each list by the Term column
    undergraduates.sort(key=lambda x: int(x['Term']))
    graduates.sort(key=lambda x: int(x['Term']))

# Print undergraduate courses
print("Undergraduate Courses:")
for course in undergraduates:
    print(course)

# Print graduate courses
print("\nGraduate Courses:")
for course in graduates:
    print(course)
